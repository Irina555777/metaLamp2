
- var objColors = [{colorClass: 'Dark-color', colorName: 'Dark Shade 100%', colorValue: '#1F2041'}, {colorClass: 'Dark-color75', colorName: 'Dark Shade 75%', colorValue: '#1F2041'},{colorClass: 'Dark-color50', colorName: 'Dark Shade 50%', colorValue: '#1F2041'},{colorClass: 'Dark-color25', colorName: 'Dark Shade 25%', colorValue: '#1F2041'},{colorClass: 'Dark-color5', colorName: 'Dark Shade 5%', colorValue: '#1F2041'},{colorClass: 'Primary-Color',colorName: 'Purple', colorValue: '#6FCF97'},{colorClass: 'Secondary-color',colorName: 'Green', colorValue: '#1F2041'}, ]  

mixin colorBlock(x,arr)
  div
    each x in objColors
      .ui-color-block-wrap.ui-color-block-mb
        .ui-color-square(class=x.colorClass)
        .ui-colot-text() 
          p.uiColorBlock-textName-style= x.colorName
          p.uiColorBlock-textValue-style= x.colorValue
          
   

    

      

    
  
